#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/mouse.h>

#define DEF 0
#define LWR 1
#define RSE 2
#define ADJ 3

&lt {
    quick-tap-ms = <200>;
    flavor = "balanced";
    tapping-term-ms = <200>;
};

&mt {
    quick-tap-ms = <200>;
    flavor = "balanced";
    tapping-term-ms = <200>;
};

/ {
    conditional_layers { compatible = "zmk,conditional-layers"; };

    behaviors {
        mkpt: mouse_key_press_tap {
            compatible = "zmk,behavior-hold-tap";
            bindings = <&kp>, <&mkp>;

            #binding-cells = <2>;
            tapping-term-ms = <200>;
            quick-tap-ms = <200>;
            flavor = "balanced";
            label = "Mouse Key Press Tap";
        };

        ktt: key_toggle_tap {
            compatible = "zmk,behavior-hold-tap";
            label = "Key Toggle Tap";
            bindings = <&kp>, <&kt>;

            #binding-cells = <2>;
            tapping-term-ms = <200>;
            quick-tap-ms = <200>;
            flavor = "balanced";
        };

        btt: bluetooth_tap {
            compatible = "zmk,behavior-hold-tap";
            label = "Bluetooth Tap";
            bindings = <&kp>, <&bt>;

            #binding-cells = <2>;
            tapping-term-ms = <200>;
            quick-tap-ms = <200>;
            flavor = "balanced";
        };

        btlt: bluetooth_layer_tap {
            compatible = "zmk,behavior-hold-tap";
            label = "Bluetooth Layer Tap";
            bindings = <&mo>, <&bt>;

            #binding-cells = <2>;
            tapping-term-ms = <200>;
            quick-tap-ms = <200>;
            flavor = "balanced";
        };
    };

    combos {
        compatible = "zmk,combos";

        gui_alt_alt {
            bindings = <&kp LGUI>;
            key-positions = <37 40>;
            timeout-ms = <100>;
        };

        mouse_middle_click {
            bindings = <&mkp MB3>;
            key-positions = <38 39>;
            timeout-ms = <100>;
        };

        change_lang_ei {
            bindings = <&kp LS(RSHIFT)>;
            key-positions = <3 8>;
            timeout-ms = <100>;
            layers = <0>;
        };

        change_lang_gh {
            bindings = <&kp LS(RSHIFT)>;
            key-positions = <17 18>;
            timeout-ms = <100>;
            layers = <0>;
        };

        change_lang_vm {
            bindings = <&kp LS(RSHIFT)>;
            key-positions = <28 31>;
            timeout-ms = <100>;
            layers = <0>;
        };

        change_lang_wo {
            bindings = <&kp LS(RSHIFT)>;
            key-positions = <2 9>;
            timeout-ms = <100>;
            layers = <0>;
        };

        shortcuts_c_23l {
            bindings = <&mo 6>;
            key-positions = <3 16>;
            layers = <0>;
            timeout-ms = <200>;
        };

        shortcuts_c_23r {
            bindings = <&mo 6>;
