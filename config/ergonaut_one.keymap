            bindings = <
&kp RIGHT_BRACKET  &mt LCTRL Q  &lt 4 W       &lt 4 E         &mt LALT R      &kp T           &kp Y           &mt LALT U      &lt 4 I         &lt 4 O       &mt RCTRL P      &kp LEFT_BRACKET
&mt LCTRL GRAVE    &lt 2 A      &mt LSHIFT S  &lt 1 D         &mt LCTRL F     &mt RALT G      &mt RALT H      &mt RCTRL J     &lt 1 K         &mt RSHIFT L  &lt 2 SEMICOLON  &mt RCTRL APOSTROPHE
&mt LSHIFT TAB     &kp Z        &kp X         &kp C           &kp V           &kp B           &kp N           &kp M           &kp COMMA       &kp DOT       &kp FSLH         &mt RSHIFT BACKSLASH
                                              &mkpt LGUI MB1  &mt LALT SPACE  &mkpt RALT MB2  &mkpt RALT MB1  &mt LALT SPACE  &mkpt RGUI MB2
            >;
        };

        qwerty_pushed {
            bindings = <
&trans  &mt LCTRL TAB  &lt 5 GRAVE   &lt 5 RIGHT_BRACKET  &mt LALT T   &trans      &trans      &mt LALT Y   &lt 5 LEFT_BRACKET  &lt 5 APOSTROPHE  &mt RCTRL BACKSLASH  &trans
&trans  &lt 3 Z        &mt LSHIFT X  &kp C                &mt LCTRL V  &mt RALT B  &mt RALT N  &mt RCTRL M  &kp COMMA           &mt RSHIFT DOT    &lt 3 SLASH          &trans
&trans  &trans         &trans        &trans               &trans       &trans      &trans      &trans       &trans              &trans            &trans               &trans
                                     &trans               &trans       &trans      &trans      &trans       &trans
